  id='experiencia'>
  <div><p class='font-bold text-[32px]'>Experiencia</p></div>
  <div class='md:flex flex-col gap-14'>
    <Proyect
      name='Pets Register'
      img='/veterinaria.webp'
      repo='https://github.com/Bogaralex135/Citas-React'
      demo='https://petsregister.netlify.app'>
      Proyecto propio desarrollado en React junto con tailwind
    </Proyect>
    <Proyect
      name='CoreCrypter'
      img='/corecrypter.webp'
      repo='https://github.com/Bogaralex135/CoreCrypter.github.io'
      demo='https://bogaralex135.github.io/CoreCrypter.github.io/'>
      Alura Challenge
    </Proyect>
  </div>
